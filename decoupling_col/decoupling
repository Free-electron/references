@Article{Pandit2016,
  Title                    = {A Compact Planar MIMO Monopole Antenna with Reduced Mutual Coupling for WLAN Applications using ELC Resonator},
  Author                   = {Pandit, S. and Mohan, A. and Ray, P.},
  Journal                  = {2016 Asia-pacific Microwave Conference (apmc2016)},
  Year                     = {2016},
  Pages                    = {IEEE, MTTS, IEEE Delhi Sect, European Microwave Assoc},

  Abstract                 = {This paper presents a novel mutual coupling reduction technique of compact two-element multiple- input multiple-output (MIMO) antenna using Electric-LC (ELC) resonator. The proposed antenna has omnidirectional radiation pattern and the 20 dB isolation bandwidth is 9.8 %, i.e., from 5.31-5.86 GHz which covers the WLAN frequency range of IEEE 802.11a standard whereas return loss of the proposed antenna is better than 15 dB throughout the entire band. The separation between antenna elements is only 0.14 lambda(0), where lambda(0) corresponds to free space wavelength at 5.7 GHz. The radiation pattern is negligibly affected and the proposed antenna has 8.5 dB lower mutual coupling than that of the reference antenna due to the ELC resonator. This concept is verified with fabrication and measurement.},
