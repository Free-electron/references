@Article{Wu2013,
  Title                    = {Single-Layer Microstrip High-Directivity Coupled-Line Coupler With Tight Coupling},
  Author                   = {Wu, Yongle and Sun, Weinong and Leung, Sai-Wing and Diao, Yinliang and Chan, Kwok-Hung and Siu, Yun-Ming},
  Journal                  = {Ieee Transactions On Microwave Theory and Techniques},
