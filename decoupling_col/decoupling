@Article{Kiani-Kharaji2018,
  Title                    = {Wide scan phased array patch antenna with mutual coupling reduction},
  Author                   = {Kiani-Kharaji, M. and Hassani, H. R. and Mohammad-Ali-Nezhad, S.},
  Journal                  = {Iet Microwaves Antennas \& Propagation},
  Year                     = {2018},

  Month                    = oct,
  Number                   = {12},
  Pages                    = {1932--1938},
  Volume                   = {12},

  __markedentry            = {[913:]},
  Abstract                 = {A simple technique to reduce the mutual coupling between patch antenna arrays is proposed. Mutual coupling phenomena in patch antenna array are investigated and according to the coupling characteristics, a simple rectangular slot(s) placed in between the patch elements is proposed. An equivalent circuit of the proposed structure is obtained through the reciprocity method. In the proposed technique, the series equivalent circuit of the slot is used to reduce the mutual coupling level without changes to the back lobe as well as cross-polarisation levels compared to conventional arrays. The mutual coupling reduction is >20dB when compared with the conventional array. Applied to a phased array antenna, the reduction in the mutual coupling results in a wider scan range, up to 75 degrees. A parametric study is carried out for different parameters of the slot that affect the mutual coupling and the scan range. A prototype of the antenna array is fabricated and the measured results are compared to the simulation.},
  Af                       = {Kiani-Kharaji, MojtabaEOLEOLHassani, Hamid RezaEOLEOLMohammad-Ali-Nezhad, Sajad},
  Doi                      = {10.1049/iet-map.2018.0155},
  Ei                       = {1751-8733},
  File                     = {Kiani-Kharaji2018.pdf:Kiani-Kharaji2018.pdf:PDF},
  Owner                    = {913},
  Sn                       = {1751-8725},
  Timestamp                = {2018.10.23},
